  $('.change-profile').click(function() {
    var h5 = $(this).closest('h5');
    $(this).find('i').show();
    $.get('/diy/recipes/chocolate-keto-liquid-cake/nutrient-profile-options', function(res) {
      h5.hide();
      $('.change-nutrient-profile').html(res).show();
    });
  });
});
