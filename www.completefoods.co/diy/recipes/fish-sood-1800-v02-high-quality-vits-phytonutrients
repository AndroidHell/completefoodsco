    $(this).find('i').show();
    $.get('/diy/recipes/fish-sood-1800-v02-high-quality-vits-phytonutrients/nutrient-profile-options', function(res) {
      h5.hide();
      $('.change-nutrient-profile').html(res).show();
    });
  });
});
