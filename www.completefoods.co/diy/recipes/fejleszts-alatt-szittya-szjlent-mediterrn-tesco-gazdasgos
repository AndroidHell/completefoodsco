    $.get('/diy/recipes/fejleszts-alatt-szittya-szjlent-mediterrn-tesco-gazdasgos/nutrient-profile-options', function(res) {
      h5.hide();
      $('.change-nutrient-profile').html(res).show();
    });
  });
});
